generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model mydishes {
  id       Int      @id @default(autoincrement())
  name     String
  date     String?
  location String?
  city     String?
  type     String?
  meal     String?
  cooked   Boolean?
}
